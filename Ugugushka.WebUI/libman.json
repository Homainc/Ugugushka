{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "bootstrap@4.4.1",
      "destination": "wwwroot/libs/bootstrap/",
      "files": [
        "dist/css/bootstrap-grid.min.css.map",
        "dist/css/bootstrap-grid.min.css"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "font-awesome@5.13.0",
      "destination": "wwwroot/libs/font-awesome/"
    },
    {
      "provider": "cdnjs",
      "library": "cloudinary-jquery@2.8.2",
      "destination": "wwwroot/libs/cloudinary-jquery/"
    },
    {
      "provider": "cdnjs",
      "library": "jquery@3.5.1",
      "destination": "wwwroot/libs/jquery/"
    },
    {
      "provider": "cdnjs",
      "library": "blueimp-load-image@5.12.0",
      "destination": "wwwroot/libs/blueimp-load-image/"
    },
    {
      "provider": "cdnjs",
      "library": "blueimp-file-upload@10.28.0",
      "destination": "wwwroot/libs/blueimp-file-upload/"
    },
    {
      "provider": "cdnjs",
      "library": "cloudinary-jquery-file-upload@2.8.2",
      "destination": "wwwroot/libs/cloudinary-jquery-file-upload/"
    }
  ]
}